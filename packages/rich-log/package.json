{
  "name": "rich-log",
  "version": "1.0.0",
  "description": "JSX + console.log = RichLog",
  "repository": "https://github.com/iam-medvedev/rich-log.git",
  "homepage": "https://github.com/iam-medvedev/rich-log#readme",
  "bugs": "https://github.com/iam-medvedev/rich-log/issues",
  "author": "Ilya Medvedev <ilya@medvedev.im>",
  "license": "WTFPL",
  "files": [
    "dist/*",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "jsx",
    "console",
    "rich",
    "log",
    "styled",
    "logging",
    "css"
  ],
  "packageManager": "yarn@3.2.1",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "build": "NODE_ENV=production parcel build",
    "minify-js": "uglifyjs --compress --mangle --source-map \"url='main.js.map'\" --output ./dist/main.js -- ./dist/main.js",
    "minify-module": "uglifyjs --compress --mangle --source-map \"url='module.js.map'\" --output ./dist/module.js -- ./dist/module.js",
    "dev": "parcel watch",
    "test": "jest --watch",
    "test:ci": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "csstype": "^3.1.0"
  },
  "devDependencies": {
    "@parcel/core": "^2.6.1",
    "@parcel/packager-ts": "^2.6.1",
    "@parcel/transformer-typescript-types": "^2.6.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@testing-library/react": "^13.3.0",
    "@types/eslint": "^8.4.3",
    "@types/jest": "^28.1.2",
    "@types/prettier": "^2.6.3",
    "@types/uglify-js": "^3.16.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "jest-fetch-mock": "^3.0.3",
    "nock": "^13.2.7",
    "parcel": "^2.6.1",
    "semantic-release": "^19.0.3",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4",
    "uglify-js": "^3.16.1"
  }
}
